{
    "id": "nodemanager207",
    "hostname": "192.168.1.207",
    "resource": {
       "vcpu": 4,
       "memory": 8092
    },
    "containers" : [{
        "image": "2breakfast/hadoop:2.7.3-yarn",
        "envs": [{
            "env": "PUBLISHED_IP",
            "value": "192.168.1.207"
        }, {
            "env": "RM_HOST",
            "value": "192.168.1.204"
        }, {
            "env": "JH_HOST",
            "value": "192.168.1.208"
        }, {
            "env": "RESOURCE_CPU_VCORE",
            "value": "4"
        }, {
            "env": "RESOURCE_MEM_MB",
            "value": "8092"
        }, {
            "env": "HDFS_NAMENODE_RPC_HOST",
            "value": "192.168.1.200"
        }, {
            "env": "ROLE",
            "value": "nodemanager"
        }],
        "volumes": [{
            "volume": "data_vol",
            "path": "/tmp/hadoop",
            "readOnly": false
        }]
    }],
    "volumes": [{
        "name": "data_vol",
        "source": "/hadoop/nm207",
        "driver": "vfs"
    }],
    "interfaces": [{
        "bridge": "hyper0",
        "ip": "192.168.1.207/24",
        "gateway": "192.168.1.20"
    }]
}
